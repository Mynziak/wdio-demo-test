{
        "name": "wdio-demo-test",
        "version": "0.0.0",
        "description": "Project for conducting E2E tests for native iOS and Web apps using the WebdriverIO framework.",
        "scripts": {

            "web.chrome.heroku": "wdio config/heroku_web_app/heroku_app.conf.ts",

            "ios_app.iphone11": "wdio config/ios_app/wdio.ios_app.iphone11.conf.ts",

            "ios_app.iphonex": "wdio config/ios_app/wdio.ios.ios_app.iphonex.conf.ts",

            "ios.safari.iphone11": "wdio config/ios_app/wdio.ios_app.iphone11.conf.ts",

            "report:generate": "npx allure generate --clean ./allure-results/ -o ./allure-report",
            "report:open": "npx allure open allure-report"
        },
        "repository": {
            "type": "git",
            "url": "git+https://github.com/Tribe-GmbH/qa-hma-test"
        },
        "keywords": [
            "WebdriverIO",
            "appium",
            "WepApp",
            "native app",
            "typescript",
            "ios"
        ],
        "dependencies": {
            "@wdio/allure-reporter": "^7.2.1",
            "allure-commandline": "^2.13.8",
            "@wdio/cli": "^7.0.0",
            "@wdio/local-runner": "^7.19.7",
            "@wdio/spec-reporter": "^7.19.7",
            "@wdio/dot-reporter": "^8.8.6",
            "chromedriver": ">=114",
            "wdio-chromedriver-service": "^7.0.0",
            "@wdio/mocha-framework": "^7.19.7",
            "@wdio/appium-service": "^8.10.5",
            "@wdio/browserstack-service": "^7.19.7",
            "@wdio/spec-reporter": "^7.19.7",
            "csv-writer": "^1.6.0",
            "jest": "^29.5"
        },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "eslint-plugin-wdio": "^7.19.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}

